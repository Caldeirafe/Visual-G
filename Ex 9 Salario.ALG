Algoritmo "semnome"

Var
nome : caractere
vhora : real
thora : real
salario : real

Inicio

      escreva("Digite seu nome: ")
      leia(nome)
      escreva("Horas trabalhadas: ")
      leia(thora)
      escreva("Valor da Hora: ")
      leia(vhora)
      salario <- thora*vhora
      escreva("Salario é de: ", salario)


Fimalgoritmo